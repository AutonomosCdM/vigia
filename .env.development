# Vigia Development Environment - Real Services Configuration
# Copy to .env and configure with your actual credentials

# === ENVIRONMENT ===
ENVIRONMENT=development
DEBUG=true
LOG_LEVEL=DEBUG

# === DATABASE CONFIGURATION ===
DATABASE_URL=postgresql://vigia_user:vigia_password@localhost:5432/vigia_dev
POSTGRES_USER=vigia_user
POSTGRES_PASSWORD=vigia_password
POSTGRES_DB=vigia_dev

# === REDIS CONFIGURATION ===
REDIS_URL=redis://localhost:6379/0
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=
REDIS_DB=0

# === CELERY CONFIGURATION ===
CELERY_BROKER_URL=redis://localhost:6379/0
CELERY_RESULT_BACKEND=redis://localhost:6379/1
CELERY_ALWAYS_EAGER=false  # Set to false for real async processing

# === SUPABASE CONFIGURATION ===
# Get these from: https://supabase.com/dashboard/project/YOUR_PROJECT/settings/api
SUPABASE_URL=YOUR_SUPABASE_PROJECT_URL
SUPABASE_KEY=YOUR_SUPABASE_ANON_KEY
SUPABASE_SERVICE_ROLE_KEY=YOUR_SUPABASE_SERVICE_ROLE_KEY

# === TWILIO WHATSAPP CONFIGURATION ===
# Get these from: https://console.twilio.com/
TWILIO_ACCOUNT_SID=YOUR_TWILIO_ACCOUNT_SID
TWILIO_AUTH_TOKEN=YOUR_TWILIO_AUTH_TOKEN
TWILIO_WHATSAPP_FROM=whatsapp:+14155238886  # Twilio sandbox number

# === SLACK CONFIGURATION ===
# Create Slack app at: https://api.slack.com/apps
SLACK_BOT_TOKEN=xoxb-YOUR-SLACK-BOT-TOKEN
SLACK_SIGNING_SECRET=YOUR_SLACK_SIGNING_SECRET
SLACK_WEBHOOK_URL=YOUR_SLACK_WEBHOOK_URL

# === SENDGRID EMAIL CONFIGURATION ===
# Get API key from: https://app.sendgrid.com/settings/api_keys
SENDGRID_API_KEY=YOUR_SENDGRID_API_KEY
SENDGRID_FROM_EMAIL=noreply@yourdomain.com

# === GOOGLE CLOUD / VERTEX AI ===
# For MedGemma and ADK integration
GOOGLE_APPLICATION_CREDENTIALS=path/to/service-account-key.json
GOOGLE_CLOUD_PROJECT=your-gcp-project-id
VERTEX_AI_REGION=us-central1

# === SECURITY KEYS ===
# Generate these with: python scripts/setup/secure_key_generator.py
JWT_SECRET_KEY=your-jwt-secret-here
ENCRYPTION_KEY=your-encryption-key-here
WEBHOOK_SECRET=your-webhook-secret-here

# === MEDICAL AI CONFIGURATION ===
MEDGEMMA_MODEL_PATH=medgemma-7b  # Ollama model name
USE_LOCAL_AI=true  # Set to true for Ollama, false for Vertex AI
MODEL_CONFIDENCE_THRESHOLD=0.25

# === MCP CONFIGURATION ===
MCP_SERVER_HOST=localhost
MCP_SERVER_PORT=8080
MCP_ENABLE_REAL_SERVICES=true

# === MONITORING ===
SENTRY_DSN=YOUR_SENTRY_DSN_URL
AGENTOPS_API_KEY=YOUR_AGENTOPS_API_KEY

# === FILE STORAGE ===
UPLOAD_FOLDER=./data/uploads
MAX_UPLOAD_SIZE=10485760  # 10MB
ALLOWED_EXTENSIONS=jpg,jpeg,png,webp

# === MEDICAL COMPLIANCE ===
MEDICAL_COMPLIANCE_LEVEL=development
PHI_PROTECTION_ENABLED=true
AUDIT_LOGGING_ENABLED=true