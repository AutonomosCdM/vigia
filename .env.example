# Vigia A2A Authentication
# Generate a secure secret key for A2A communication between medical agents
# Use: openssl rand -hex 32
VIGIA_A2A_SECRET_KEY=your_secure_32_char_secret_key_here

# Database Configuration
DATABASE_URL=postgresql://user:password@localhost:5432/vigia_db

# Redis Configuration  
REDIS_URL=redis://localhost:6379

# Slack Integration (for notifications)
SLACK_BOT_TOKEN=xoxb-your-slack-bot-token
SLACK_CHANNEL=#vigia-alerts

# Supabase Configuration
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key

# AgentOps (for monitoring)
AGENTOPS_API_KEY=your_agentops_api_key

# Medical AI Configuration
OPENAI_API_KEY=your_openai_key
ANTHROPIC_API_KEY=your_anthropic_key

# Security Settings
ENCRYPTION_KEY=your_fernet_encryption_key
JWT_SECRET_KEY=your_jwt_secret_key

# Application Settings
ENVIRONMENT=development
DEBUG=true
LOG_LEVEL=INFO